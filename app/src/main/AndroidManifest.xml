<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.menan.micropenny">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.menan.micropenny.StatusActivity"></activity>
        <activity android:name="com.menan.micropenny.ReferralActivity" />
        <activity android:name="com.menan.micropenny.SupportActivity" />
        <activity android:name="com.menan.micropenny.WithdrawActivity" />
        <activity android:name="com.menan.micropenny.FacebookActivity" />
        <activity android:name="com.menan.micropenny.HomeActivity" />
        <activity
            android:name="com.adcolony.sdk.AdColonyInterstitialActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:hardwareAccelerated="true" />
        <activity android:name="com.menan.micropenny.RegisterActivity" />
        <activity android:name="com.menan.micropenny.LoginActivity" />
        <activity android:name="com.menan.micropenny.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.apptracker.android.module.AppModuleActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            android:hardwareAccelerated="false"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" />

        <service android:name="com.apptracker.android.track.AppTrackerService" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-4671840559976910~3251448717" />
    </application>

</manifest>